            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Utf8StringInterpolation" Version="1.3.1"/>
    </ItemGroup>

</Project>
