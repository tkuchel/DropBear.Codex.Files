        <PackageReference Include="DropBear.Codex.Serialization" Version="2024.3.3"/>
        <PackageReference Include="DropBear.Codex.StateManagement" Version="2024.3.5"/>
        <PackageReference Include="DropBear.Codex.Utilities" Version="2024.3.8"/>
        <PackageReference Include="DropBear.Codex.Validation" Version="2024.3.1"/>
        <PackageReference Include="Meziantou.DotNet.CodingStandard" Version="1.0.119">
